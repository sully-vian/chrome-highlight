{
    "manifest_version": 3,
    "name": "Medium Rare",
    "version": "1.0",
    "permissions": [
        "storage",
        "scripting",
        "notifications"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "vendor/*.min.css"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}
